<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>


<VBox alignment="TOP_CENTER" prefHeight="500.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1" fx:controller="poorsa.org.frontend.ContactInfoController">
   <children>
      <Label text="enter your contact info:">
         <font>
            <Font size="24.0" />
         </font>
         <VBox.margin>
            <Insets left="-450.0" top="20.0" />
         </VBox.margin>
      </Label>
      <HBox alignment="CENTER" maxHeight="200.0" prefHeight="200.0">
         <children>
            <VBox alignment="TOP_CENTER" minWidth="400.0" prefHeight="280.0" prefWidth="400.0">
               <children>
                  <TextArea fx:id="address" maxHeight="120.0" maxWidth="300.0" prefHeight="95.0" prefWidth="200.0" promptText="Address">
                     <padding>
                        <Insets left="10.0" />
                     </padding>
                     <VBox.margin>
                        <Insets top="5.0" />
                     </VBox.margin>
                  </TextArea>
                  <TextField id="username" fx:id="email" maxHeight="45.0" maxWidth="300.0" prefHeight="45.0" prefWidth="300.0" promptText="email">
                     <padding>
                        <Insets left="15.0" />
                     </padding>
                     <VBox.margin>
                        <Insets top="5.0" />
                     </VBox.margin>
                  </TextField>
                  <TextField id="username" fx:id="pnh" maxHeight="45.0" maxWidth="300.0" prefHeight="45.0" prefWidth="300.0" promptText="phone number (home)">
                     <padding>
                        <Insets left="15.0" />
                     </padding>
                     <VBox.margin>
                        <Insets top="5.0" />
                     </VBox.margin>
                  </TextField>
                  <TextField id="username" fx:id="pnw" maxHeight="45.0" maxWidth="300.0" prefHeight="45.0" prefWidth="300.0" promptText="phone number (work)">
                     <padding>
                        <Insets left="15.0" />
                     </padding>
                     <VBox.margin>
                        <Insets top="5.0" />
                     </VBox.margin>
                  </TextField>
               </children>
               <HBox.margin>
                  <Insets />
               </HBox.margin>
            </VBox>
            <VBox alignment="TOP_CENTER" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="300.0" prefWidth="400.0">
               <children>
                  <TextField id="username" fx:id="pnp" maxHeight="45.0" maxWidth="300.0" prefHeight="45.0" prefWidth="300.0" promptText="phone number (personal)">
                     <padding>
                        <Insets left="15.0" />
                     </padding>
                     <VBox.margin>
                        <Insets top="5.0" />
                     </VBox.margin>
                  </TextField>
                  <TextField id="username" fx:id="sm1" maxHeight="45.0" maxWidth="300.0" prefHeight="45.0" prefWidth="300.0" promptText="social media 1">
                     <padding>
                        <Insets left="15.0" />
                     </padding>
                     <VBox.margin>
                        <Insets top="5.0" />
                     </VBox.margin>
                  </TextField>
                  <TextField id="username" fx:id="sm2" maxHeight="45.0" maxWidth="300.0" prefHeight="45.0" prefWidth="300.0" promptText="social media 2">
                     <padding>
                        <Insets left="15.0" />
                     </padding>
                     <VBox.margin>
                        <Insets top="5.0" />
                     </VBox.margin>
                  </TextField>
                  <TextField id="username" fx:id="sm3" maxHeight="45.0" maxWidth="300.0" prefHeight="45.0" prefWidth="300.0" promptText="social media 3">
                     <padding>
                        <Insets left="15.0" />
                     </padding>
                     <VBox.margin>
                        <Insets top="5.0" />
                     </VBox.margin>
                  </TextField>
                  <TextField id="username" fx:id="website" layoutX="60.0" layoutY="165.0" maxHeight="45.0" maxWidth="300.0" prefHeight="45.0" prefWidth="300.0" promptText="website">
                     <padding>
                        <Insets left="15.0" />
                     </padding>
                     <VBox.margin>
                        <Insets top="5.0" />
                     </VBox.margin>
                  </TextField>
               </children>
            </VBox>
         </children>
         <VBox.margin>
            <Insets top="30.0" />
         </VBox.margin>
      </HBox>
      <Label fx:id="resultLabel">
         <VBox.margin>
            <Insets bottom="15.0" top="-20.0" />
         </VBox.margin>
      </Label>
      <HBox alignment="CENTER" prefHeight="50.0" prefWidth="500.0">
         <children>
            <Button id="createB" fx:id="confirm" mnemonicParsing="false" onAction="#confirmOnAction" prefHeight="45.0" prefWidth="185.0" text="confirm" textFill="WHITE">
               <HBox.margin>
                  <Insets top="10.0" />
               </HBox.margin>
               <font>
                  <Font name="Comic Sans MS" size="15.0" />
               </font>
            </Button>
            <Button id="backB" fx:id="back" mnemonicParsing="false" onAction="#backOnAction" prefHeight="45.0" prefWidth="185.0" text="back" textFill="WHITE">
               <HBox.margin>
                  <Insets left="30.0" top="10.0" />
               </HBox.margin>
               <font>
                  <Font name="Comic Sans MS" size="15.0" />
               </font>
            </Button>
         </children>
         <VBox.margin>
            <Insets />
         </VBox.margin>
      </HBox>
   </children>
</VBox>
